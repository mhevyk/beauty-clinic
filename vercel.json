{
  "rewrites": [
    { "source": "/storybook", "destination": "/storybook-static/index.html" },
    { "source": "/storybook/(.*)", "destination": "/storybook-static/$1" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "builds": [
    {
      "src": "dist/storybook-static/**/*",
      "use": "@vercel/static"
    }
  ]
}